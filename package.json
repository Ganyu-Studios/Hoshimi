{
	"name": "hoshimi",
	"version": "0.1.0",
	"description": "A Shoukaku wrapper highly inspired in Kazagumo.",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4",
	"files": ["dist"],
	"lint-staged": {
		"*.{ts,json}": ["biome format --write ."]
	},
	"repository": {
		"url": "https://github.com/Ganyu-Studios/Hoshimi.git",
		"type": "git"
	},
	"scripts": {
		"build": "tsup",
		"lint": "biome lint ./src",
		"typecheck": "tsc --noEmit",
		"pkg:prepublish": "pnpm build && tsx ./scripts/prepublish.mts && cd .npm && npm pack",
		"pgk:publish": "cd .npm && npm publish",
		"format": "biome format --write .",
		"prepare": "pnpm build && husky"
	},
	"keywords": [
		"discord",
		"lavalink",
		"shoukaku",
		"discord-player",
		"discord.js",
		"seyfert",
		"kazagumo",
		"distube",
		"music-bot",
		"lavalink-player",
		"lavalink-client",
		"music"
	],
	"author": "Ganyu Studios",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"@types/node": "^22.7.5",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"shoukaku": "github:shipgirlproject/Shoukaku",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3",
		"vitest": "^2.1.3"
	}
}
