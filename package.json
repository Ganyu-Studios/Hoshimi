{
	"name": "hoshimi",
	"version": "0.1.0",
	"description": "A Shoukaku wrapper highly inspired in Kazagumo.",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c",
	"lint-staged": {
		"*.ts": [
			"biome format --write ."
		]
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"lint": "biome lint ./src",
		"pkg:prepublish": "pnpm build && tsx ./scripts/prepublish.mts && cd .npm && npm pack",
		"pgk:publish": "cd .npm && npm publish",
		"format": "biome format --write .",
		"prepare": "pnpm build && husky"
	},
	"keywords": [
		"discord",
		"lavalink",
		"shoukaku",
		"discord-player",
		"discord.js",
		"seyfert",
		"kazagumo",
		"distube",
		"music-bot",
		"lavalink-player",
		"lavalink-client",
		"music"
	],
	"author": "Ganyu Studios",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.9.1",
		"@types/node": "^22.5.5",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"shoukaku": "github:shipgirlproject/Shoukaku",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2",
		"vitest": "^2.1.1"
	}
}
