{
	"name": "hoshimi",
	"version": "0.1.0",
	"description": "A Shoukaku wrapper highly inspired in Kazagumo.",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca",
	"lint-staged": {
		"*.{ts,json}": ["biome format --write ."]
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"lint": "biome lint ./src",
		"typecheck": "tsc --noEmit",
		"pkg:prepublish": "pnpm build && tsx ./scripts/prepublish.mts && cd .npm && npm pack",
		"pgk:publish": "cd .npm && npm publish",
		"format": "biome format --write .",
		"prepare": "pnpm build && husky"
	},
	"keywords": [
		"discord",
		"lavalink",
		"shoukaku",
		"discord-player",
		"discord.js",
		"seyfert",
		"kazagumo",
		"distube",
		"music-bot",
		"lavalink-player",
		"lavalink-client",
		"music"
	],
	"author": "Ganyu Studios",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"@types/node": "^22.7.5",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"shoukaku": "github:shipgirlproject/Shoukaku",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3",
		"vitest": "^2.1.2"
	}
}
